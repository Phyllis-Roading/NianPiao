    </style>

    <style name="myStyle.WhiteNormalText" parent="@style/myStyle">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="myStyle.WhiteBigText" parent="@style/myStyle">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="myStyle.BlackNormalText" parent="@style/myStyle">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff333333</item>
    </style>
    
    <style name="mine">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">#666666</item>
    </style>
    <style name="mine_middle_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="history_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666666</item>
    </style>

</resources>